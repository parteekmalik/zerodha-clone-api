{
    "name": "express-api-starter-ts",
    "version": "1.2.0",
    "description": " A basic starter for an express.js API with Typescript",
    "main": "src/index.ts",
    "scripts": {
        "dev": "nodemon --exec ts-node src/server.ts",
        "dev1": "nodemon --exec ts-node src/backend/server.ts",
        "start:server": "ts-node src/server.ts",
        "start:server1": "ts-node src/backend/server.ts",
        "start": "concurrently \"npm run start:server1\" \"npm run start:server\" ",
        "startdev": "concurrently \"npm run start:dev\" \"npm run start:dev1\" ",
        "build": "tsc",
        "lint": "eslint --fix src test",
        "test": "jest",
        "db:push": "prisma db push",
        "db:studio": "prisma studio",
        "typecheck": "tsc --noEmit",
        "postinstall": "prisma generate",
        "migrate": "prisma migrate dev"
    },
    "keywords": [],
    "author": "CJ R. <cj@null.computer> (https://w3cj.now.sh)",
    "repository": {
        "type": "git",
        "url": "https://github.com/w3cj/express-api-starter.git"
    },
    "license": "MIT",
    "dependencies": {
        "axios": "^1.6.2",
        "body-parser": "^1.20.2",
        "cors": "^2.8.5",
        "dotenv": "^16.3.1",
        "express": "^4.18.2",
        "lodash": "^4.17.21",
        "moment": "^2.29.4",
        "next-auth": "^4.24.7",
        "socket.io": "^4.7.5",
        "ws": "^8.16.0",
        "zlibjs": "^0.3.1",
        "zod": "^3.22.4"
    },
    "devDependencies": {
        "@prisma/client": "^5.10.2",
        "@types/cors": "^2.8.13",
        "@types/decompress": "^4.2.7",
        "@types/express": "^4.17.17",
        "@types/jest": "^29.5.2",
        "@types/lodash": "^4.14.199",
        "@types/node": "^20.11.25",
        "@types/pg": "^8.10.7",
        "@types/supertest": "^2.0.12",
        "@types/ws": "^8.5.6",
        "@typescript-eslint/eslint-plugin": "^5.59.9",
        "@typescript-eslint/parser": "^5.59.9",
        "concurrently": "^8.2.1",
        "eslint": "^8.42.0",
        "eslint-config-airbnb-typescript": "^17.0.0",
        "eslint-import-resolver-typescript": "^3.5.5",
        "eslint-plugin-import": "^2.27.5",
        "helmet": "^7.0.0",
        "jest": "^29.5.0",
        "mongodb": "^6.1.0",
        "nodemon": "^3.0.1",
        "prisma": "^5.10.2",
        "supertest": "^6.3.3",
        "ts-jest": "^29.1.0",
        "ts-node": "^10.9.2",
        "typescript": "^5.4.2"
    }
}
